{
    "extends": ["airbnb", "prettier"],
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "prettier"
    ],
    "rules": {
        "import/no-unresolved": "off",
        "import/prefer-default-export": "off",
        "react/jsx-filename-extension" : "off",
        "react/prop-types": "off"
    },
    "env": {
        "browser": true
    }
}